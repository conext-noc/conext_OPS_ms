option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: ms_ops.wsgi:application

files:
  “/etc/nginx/conf.d/01-timeout.conf”:
     mode: “000644”
     owner: root
     group: root
     content: |
       keepalive_timeout s;
       proxy_connect_timeout 19000s;
       proxy_send_timeout 19000s; 
       proxy_read_timeout 19000s; 
       fastcgi_send_timeout 19000s; 
       fastcgi_read_timeout 19000s;
container_commands:
  nginx_reload:
     command: “sudo service nginx reload”